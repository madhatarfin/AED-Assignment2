    public String getPatientName() {
        return this.getPatientName();

    }

    public String  getID() {
        return this.getID();

    }
    public void printPatient(){
        //System.out.println(this.person.printID() + ":" + this.person.printName());
        System.out.println(this.getPatientName());
    }
    
}
